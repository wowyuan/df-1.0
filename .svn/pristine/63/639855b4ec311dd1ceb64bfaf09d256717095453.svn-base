/*index*/
/* Header */
#head { background:#181015 url( ../images/bg_header.jpg) no-repeat; background-size: cover; min-height:520px; text-align: center; padding-top:240px; color:white; font-family:"Open sans", Helvetica, Arial; font-weight:300; }
#head.secondary { height:100px; min-height: 100px; padding-top:0px; }
#head .lead { font-family:"Open sans", Helvetica, Arial; font-size:44px; margin-bottom:6px; color:white; line-height:1.15em; } 
#head .tagline { color:rgba(255,255,255,0.75); margin-bottom:25px; }
#head .tagline a { color:#fff; } 
#head .btn { margin-bottom:10px;}
#head .btn-default { text-shadow: none; background:transparent; color:rgba(255,255,255,.5); -webkit-box-shadow:inset 0px 0px 0px 3px rgba(255,255,255,.5); -moz-box-shadow:inset 0px 0px 0px 3px rgba(255,255,255,.5); box-shadow:inset 0px 0px 0px 3px rgba(255,255,255,.5); background: transparent; }
#head .btn-default:hover,
#head .btn-default:focus { color:rgba(255,255,255,.8); -webkit-box-shadow:inset 0px 0px 0px 3px rgba(255,255,255,.8); -moz-box-shadow:inset 0px 0px 0px 3px rgba(255,255,255,.8); box-shadow:inset 0px 0px 0px 3px rgba(255,255,255,.8); background: transparent; }
#head .btn-default:active, 
#head .btn-default.active { color:#fff; -webkit-box-shadow:inset 0px 0px 0px 3px #fff; -moz-box-shadow:inset 0px 0px 0px 3px #fff; box-shadow:inset 0px 0px 0px 3px #fff; background: transparent; }
@media (max-width: 767px) {
	#head { min-height:420px; padding-top:160px; }
	#head .lead { font-size: 34px; }
}

/* Autohide navbar */
.slideUp { top:-100px; }
.headroom { -webkit-transition: all 0.4s ease-out; -moz-transition: all 0.4s ease-out; -o-transition: all 0.4s ease-out; transition: all 0.4s ease-out; }
/* Highlights (in jumbotron in most cases) */
.highlight { margin-top:40px; }
.h-caption { text-align: center; } 
.h-caption i { display:block; font-size: 54px; color:#382526; margin-bottom:36px; }
.h-caption h4 { color:#382526; font-size: 16px; font-weight: bold; margin-bottom:20px; }
.h-body { }
/* Typography */
h1, h2, h3, h4, h5, h6 { font-family:"Open sans", Helvetica, Arial; }
h1, .h1, h2, .h2, h3, .h3 { margin-top:30px; } 
blockquote { font-style: italic; font-family: Georgia; color:#999; margin:30px 0 30px; }
label { color: #777; }
textarea{resize: none;}
.thin { font-weight:300; }
.page-title { margin-top:20px; font-weight:300; }
.text-muted { color:#888; text-indent: 3rem;}
.breadcrumb { background:none; padding:0; margin:30px 0 0px 0; }
ul.list-spaces li{ margin-bottom:10px; }
/* Helpers */
.container-full { margin: 0 auto; width: 100%; }
.top-space { margin-top: 60px; }
.top-margin { margin-top:20px; }
img { max-width:100%; }
img.pull-right { margin-left: 10px; }
img.pull-left { margin-right: 10px; }
#map { width:100%; height:280px; }
#social { margin-top:50px; margin-bottom:50px; }
#social .wrapper { width:340px; margin:0 auto; }
/* Main content block */
.maincontent { }
/* Sidebars */
.sidebar { padding-top:135px; padding-bottom:30px; }
.sidebar .widget { margin-bottom:20px; color: #666666;}
.sidebar h1, .sidebar .h1, .sidebar h2, .sidebar .h2, .sidebar h3, .sidebar .h3 { margin-top:20px; } 
/* Footer */
.footer1 { background: #232323; padding: 30px 0 0 0; font-size: 12px; color: #999; }
.footer1 a { color: #ccc; }
.footer1 a:hover {color: #fff; }
.footer1 .widget { margin-bottom:30px; }		
.footer1 .widget-title { font-size: 17px; font-weight: bold; color: #ccc; margin: 0 0 20px; }
.footer1 .entry-meta { border-top: 1px solid #ccc; border-bottom: 1px solid #ccc; margin: 0 0 35px 0; padding: 2px 0; color: #888888; font-size: 12px; font-size: 0.75rem; }
.footer1 .entry-meta a { color: #333333; }
.footer1 .entry-meta .meta-in { border-top: 1px solid #ccc; border-bottom: 1px solid #ccc; padding: 10px 0; }
.follow-me-icons { font-size:30px; }		
.follow-me-icons i { float:left;/* margin:0 10px 0 0;*/ }		
.footer2 { background: #191919; padding: 15px 0; color: #777; font-size: 12px; }
.footer2 a { color: #aaa; }
.footer2 a:hover { color: #fff; }
.footer2 p { margin: 0; }
.widget-simplenav { margin-left:-5px; }		
.widget-simplenav a{ margin:0 5px; }		
	
.icon{background: url(../images/icon/wechat.png) no-repeat;width: 30px;height: 30px;display: inline-block;}
.icon:hover{background: url(../images/icon/wechat-hover.png) no-repeat;width: 30px;height: 30px;display: inline-block;transition: 0.5s;}

/*产品方案*/
/*DF-OPEN*/
/* section systemhead */
#DF-open-head { background: url(../images/systemhead.jpg) no-repeat; background-size: cover; min-height:520px; position: relative;}
#DF-open-head > .container > img{position: absolute;left: 60%;transform: translate(-60%);bottom:-30%}
.introduce .content h2{font-size:3.3rem;padding-top: 150px;text-align: center;}
.introduce .content span{font-size: 1.8rem;color: #888;margin: 25px 0;display: block;text-align: center;}
.introduce .content p{text-indent: 3rem;}
.introduce .content .down{width: 46px;height: 46px;border-radius: 50%; box-shadow: 0 2px 9px #afe4f7; margin: 60px auto; background: url(../images/icon/arrow_blue_down.png) no-repeat 14px 20px; transition: 0.2s;}
.fatures{background: #F8F9FB;}
.fatures .container .left{padding: 100px 0;}
.fatures .container .right img{padding: 80px 0;}
.fatures .container .left h2{font-size:3.3rem;padding-top: 40px;}
.fatures .container .left span{font-size: 1.8rem;color: #888;margin-bottom: 25px;display: block}
.fatures .container .left ul li{list-style: none;color: #666;line-height: 25px;}
.support .container .border {border: 1px solid rgba(107, 141, 199, 0);transition:0.5s;box-shadow: 0px 2px 9px #CEF3FF;margin-top: 100px;position: relative}
.support .container .left,.point,.right{padding: 50px 0;}
.support .container img{padding-bottom: 20px;}
.support .container .point{width: 50px;height: 10px;margin: 0 auto;padding-top: 100px;}
.support .container .point div{width: 5px;height: 5px;border-radius: 6px;background: #206EB0;margin:0 5px;float: left;}
@media (max-width: 767px) {
	#DF-open-head {min-height: 300px;}
	.introduce .content h2{padding-top: 100px;}
	.introduce .content span{margin: 10px 0;}
	.introduce .content .down {margin: 30px auto;}
	.fatures .container .left {padding: 0;}
	.fatures .container .right img{padding: 0;}
	.fatures .container .left h2{padding-top: 10px;text-align: center}
	.fatures .container .left span{margin: 0 auto 5px auto;text-align: center;}
	.fatures .container .left ul li{font-size: -0.4rem;}
	.support .container .border {margin-top: 50px;}
	.support .container .left,.point,.right{padding: 10px 0;}
	.support .container .point{padding: 30px 0;}
	.sidebar{padding-top: 0;}
}
/*DF-CMMC*/
#DF-cmmc-head { background: url(../images/platform_head.jpg) no-repeat; background-size: cover; min-height:420px; position: relative;}
#DF-cmmc-head .container > div{position: absolute;top: 30%;right: 20%}
#DF-cmmc-head .container > div > h2{font-size: 3.5rem;color: #2b8db5;margin-bottom: 20px;text-align: right}
.platform .content h2{font-size:3.3rem;padding-top: 150px;text-align: center;}
.platform .content span{font-size: 1.8rem;color: #888;margin: 25px 0;display: block;text-align: center;}
.platform .content p{text-indent: 3rem;}
.platform .content .down{width: 46px;height: 46px;border-radius: 50%; box-shadow: 0 2px 9px #afe4f7; margin: 60px auto; background: url(../images/icon/arrow_blue_down.png) no-repeat 14px 20px; transition: 0.2s;}
.plat_fatures{background: #F8F9FB;}
.plat_fatures .container .left img{padding: 20px 0}
.plat_fatures .container .right h2{font-size:3.3rem;padding-top: 40px;}
.plat_fatures .container .right span{font-size: 1.8rem;color: #888;margin-bottom: 25px;display: block}
.plat_fatures .container .right li{list-style: none;color: #666;line-height: 25px;}
.icon-content .container .icon-first,.icon-second,.icon-last{/*width: 85%;*/margin: 100px auto;border: 1px solid rgba(107, 141, 199, 0);transition:0.5s;box-shadow: 0px 2px 9px #CEF3FF;min-height: 400px;margin-top: 100px;}
.icon-content .container .icon-first:hover,.icon-second:hover,.icon-last:hover{border: 1px solid #6b8cc7}
.icon-content .container .icon-first dt,.icon-second dt,.icon-last dt{color:#115baa;font-size: 1.8rem;text-align: center;margin: 200px 0 20px 0;}
.icon-content .container .icon-first dd,.icon-second dd,.icon-last dd{color:#666;font-size: 1.4rem;line-height: 20px;text-indent: 3rem;transition:0.5s;padding: 0 50px;}
.icon-content .container .icon-first{background: url(../images/icon/listen.png) no-repeat;background-position: 50% 15%;}
.icon-content .container .icon-second{background: url(../images/icon/user_asset.png)no-repeat;background-position: 50% 15%;}
.icon-content .container .icon-last{background: url(../images/icon/work_order.png)no-repeat;background-position: 50% 15%;}
.icon-content .container .icon-left{background: url(../images/icon/set_secure.png)no-repeat;background-position: 10px;}
.icon-content .container .icon-right{background: url(../images/icon/set_efficient.png)no-repeat;background-position: 10px;}
.icon-content .container .icon-left dt,.icon-right dt{color:#115baa;font-size: 1.8rem;padding: 35px 0 10px 0;}
.icon-content .container .icon-left,.icon-right{padding:0 10px 10px 120px;height: 160px;border: 1px solid rgba(107, 141, 199, 0);transition:0.5s;box-shadow: 0px 2px 9px #CEF3FF;}
.icon-content .container .icon-left:hover,.icon-right:hover{border: 1px solid #6b8cc7}
@media (max-width: 767px) {
	#DF-cmmc-head .container > div{right: 0;}
	#DF-cmmc-head .container > div > h2{font-size: 2.5rem;}
	.platform .content h2{padding-top: 30px;}
	.plat_fatures .container .right {text-align: center;}
	.plat_fatures .container .right h2{padding-top: 0;}
	.icon-content .container .icon-first, .icon-second, .icon-last{margin: 20px auto;}
	.icon-content .container .icon-left, .icon-right{margin: 20px 0;}
	.icon-content .container .icon-first, .icon-second, .icon-last{min-height: 300px;}
	.icon-content .container .icon-first dt, .icon-second dt, .icon-last dt{margin: 180px 0 20px 0;}
	.icon-content .container .icon-first dd, .icon-second dd, .icon-last dd{padding: 0 10px;}
}
