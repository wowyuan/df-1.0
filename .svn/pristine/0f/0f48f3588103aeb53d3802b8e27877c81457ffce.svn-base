.timestatus > .container > .content > .status > span:nth-child(4){top: 100px;left: 900px;}
.contact > .container  .left > h2 {/*color:#2B8DB5;*/font-size:3.3rem; ;}
/*.contact > .container  .left > .us-line{border-bottom:2px solid #2B8DB5;width:46px;display: block;margin-bottom: 20px;}*/
.contact > .container  .left > span{font-size: 1.7rem;color: #888;}
.contact > .container  .left > .box{margin-top: 30px}
.contact > .container  .left > .box > ul > li{font-size: 1.4rem;list-style: none;padding-bottom: 10px}
@media (max-width: 767px) {
	#aboutus-head { min-height:420px; padding-top:160px; }
	#aboutus-head .about-img{padding-top: 0px;}
    .aboutus > .container > .content > h1{font-size:3.3rem;padding: 30px 0 0 0}
	.timestatus > .container > .content > h2{font-size: 3.6rem; ;}
	.timestatus {padding-bottom: 0px;}
	.timestatus > .container > .content > p{margin-top: 30px;margin-bottom:100px;}
	.timestatus > .container > .content > .status{height: 380px;}
	.contact > .container  .left > h2 {text-align:center; ;}
	.contact > .container  .left > .us-line{margin: 0 auto;}
	.contact > .container  .left > span{display: inherit;text-align: center;}
