/* contact  start 联系我们*/
.contactushead{position: relative}
.contactushead > .phone{margin: 0 auto;width: 576px;height: 230px;position: absolute;z-index: 11;top:50%;left: 50%;transform:translate(-50%,-50%);background: #43bbe8;color: #fff}
.contactushead > .phone > p {text-align: center;}
.contactushead > .phone > p:nth-child(1) {font-size: 4rem;font-weight: bold;margin-top: 58px;}
.contactushead > .phone > p:nth-child(2) {font-size: 2rem;margin: 20px 0}
.contactushead > .phone > span {font-size: 1.4rem;text-align: center;display: inline-block;width: 100%;position: relative}
/*.contactushead > .phone > span::before {width: 60px;border-bottom: 1px solid #fff;display: block;content: " ";position: absolute;top:7px;left: 130px}*/
/*.contactushead > .phone > span::after {width: 60px;border-bottom: 1px solid #fff;display: block;content: " ";position: absolute;top:7px;left: 383px}*/
.contactushead > .phone > .triangle{width: 0;height: 0;border-left: 10px solid transparent;border-right: 10px solid transparent;border-top: 16px solid #43bbe8;position: absolute;bottom: -16px;transform: translate(-50%);left: 50%}
.contactushead > .black{position: absolute;z-index: 10;background:#000;opacity:.3;width: 100%;height: 640px;}
